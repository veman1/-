page {
  padding: 0 0 98rpx;
}

text {
  display: block;
  padding: 30rpx;
  line-height: 2;
}
text::before {
  content: '公司简介';
  display: block;
  font-size: 1.2em;
}

view {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #000;
  color: #fff;
  font-size: 32rpx;
}
view:before { content: '一键地址导航'; white-space: nowrap; }