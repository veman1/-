.g-banner {
  height: 404rpx;
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.g-banner:before {
  background-image: url(data:image/jpeg;base64,);
  content: '';
  position: absolute;
  z-index: -1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  filter: blur(3px);
}
.g-banner image {
  width: 218rpx;
  height: 218rpx;
  border-radius: 50%;
  margin-bottom: 30rpx;
}
.g-banner text {
  font-size: 50rpx;
  color: #fff;
}

.menu { margin: 30rpx 0 0; }
.menu .item {
  position: relative;
  height: 120rpx;
  padding: 0 30rpx;
  border-bottom: 1px solid #262626;
  color: #fff;
  background: #030303;
  display: flex;
  align-items: center;
}
.menu .item image {
  width: 1.3em;
  height: 1.3em;
  margin: 0 1em 0 0;
}
.menu .item:after {
  content: '';
  position: absolute;
  right: 25rpx;
  top: 50%;
  width: .5em;
  height: .5em;
  border-left: 1px solid #fff;
  border-bottom: 1px solid #fff;
  transform-origin: 50% 50%;
  transform: translate(0, -50%) rotate(-135deg);
}