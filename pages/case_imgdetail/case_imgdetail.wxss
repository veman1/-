
page {
  padding: 0 0 98rpx;
}

.g-container {
  position: relative;
}
.g-container image {
  display: block;
  width: 100%;
  height: 0;
}
.g-container view {
  position: absolute;
  background-image: url(data:image/png;base64,);
  width: 60rpx;
  height: 60rpx;
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center;
}

.g-fixbottom {
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  background-color: rgba(0, 0, 0, .35)
}

.u-button-plain {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 3px;
  height: 3em;
  width: 380rpx;
  color: #fff;
  border: 1px solid #fff;
  box-sizing: border-box;
}
.u-button-plain:before {
  content: '预约免费设计'
}